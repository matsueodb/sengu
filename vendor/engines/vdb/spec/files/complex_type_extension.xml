<xsd:complexType name="有体物型"><xsd:annotation><xsd:documentation xml:lang="ja">有体物を表現するためのデータ型。</xsd:documentation><xsd:documentation xml:lang="en">A data type for a physical object.</xsd:documentation><xsd:appinfo><imi-info:mapping>{http://niem.gov/niem/niem-core/2.0}TangibleItemType</imi-info:mapping></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="ic:物型"><xsd:sequence><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_分類"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_色"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_製造者"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_製造日"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_数量"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_重量"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_サイズ"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_幅"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_高さ"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:有体物_長さ"/></xsd:sequence></xsd:extension></xsd:complexContent></xsd:complexType>
<xsd:complexType name="ID型"><xsd:annotation><xsd:documentation xml:lang="ja">証明を表現するためのデータ型。</xsd:documentation><xsd:documentation xml:lang="en">A data type for a representation of an identity.</xsd:documentation><xsd:appinfo><imi-info:mapping>{http://niem.gov/niem/niem-core/2.0}IdentificationType</imi-info:mapping></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="s:ComplexObjectType"><xsd:sequence><xsd:element maxOccurs="1" minOccurs="0" ref="ic:ID_識別ID"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:ID_ID種類"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:ID_発行日"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:ID_失効日"/><xsd:element maxOccurs="unbounded" minOccurs="0" ref="ic:ID_発行者"/></xsd:sequence></xsd:extension></xsd:complexContent></xsd:complexType>
